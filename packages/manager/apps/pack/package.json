{
  "name": "@ovh-ux/manager-pack-app",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "build": "webpack",
    "cypress:open": "cypress open",
    "dev": "webpack-dev-server",
    "dev:watch": "yarn run dev",
    "start": "lerna exec --stream --scope='@ovh-ux/manager-pack-app' --include-filtered-dependencies -- yarn run build",
    "start:dev": "lerna exec --stream --scope='@ovh-ux/manager-pack-app' --include-filtered-dependencies -- yarn run dev",
    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/manager-pack-app' --include-filtered-dependencies -- yarn run dev:watch",
    "test:e2e": "lerna exec --scope @ovh-ux/manager-pack-app -- node \\$LERNA_ROOT_PATH/scripts/webpack-cypress"
  },
  "dependencies": {
    "@ovh-ux/manager-core": "^1.2.0",
    "@ovh-ux/manager-pack": "^0.0.0",
    "@ovh-ux/manager-telecom-styles": "^2.0.2",
    "@ovh-ux/ng-uirouter-title": "^2.0.0-beta.3",
    "@ovh-ux/ng-translate-async-loader": "^2.0.0",
    "@ovh-ux/ng-ovh-telecom-universe-components": "^2.0.1",
    "@uirouter/angularjs": "^1.0.15",
    "angular": "^1.7.5",
    "angular-translate": "^2.18.1",
    "oclazyload": "^1.1.0",
    "ovh-api-services": "^3.24.0",
    "ovh-ui-angular": "^2.21.4",
    "ovh-ui-kit": "^2.23.1",
    "ovh-ui-kit-bs": "^2.1.1",
    "angular-ui-bootstrap": "~1.3.3",
    "@ovh-ux/ng-pagination-front": "^8.0.0-alpha.0",
    "@ovh-ux/ng-q-allsettled": "^1.0.0-alpha.0",
    "@ovh-ux/ng-ovh-simple-country-list": "git://github.com/ovh-ux/ovh-angular-simple-country-list#refactor/rollup"
  },
  "devDependencies": {
    "@ovh-ux/manager-webpack-config": "^3.0.1",
    "cypress": "^3.1.3",
    "eslint-plugin-cypress": "^2.1.3",
    "webpack-merge": "^4.1.4"
  }
}

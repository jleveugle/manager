{
  "name": "@ovh-ux/manager-telephony-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "webpack",
    "cypress:open": "cypress open",
    "dev": "webpack-dev-server",
    "dev:watch": "yarn run dev",
    "start": "lerna exec --stream --scope='@ovh-ux/manager-telephony-app' --include-filtered-dependencies -- yarn run build",
    "start:dev": "lerna exec --stream --scope='@ovh-ux/manager-telephony-app' --include-filtered-dependencies -- yarn run dev",
    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/manager-telephony-app' --include-filtered-dependencies -- yarn run dev:watch",
    "test:e2e": "lerna exec --scope @ovh-ux/manager-telephony-app -- node \\$LERNA_ROOT_PATH/scripts/webpack-cypress"
  },
  "dependencies": {
    "@ovh-ux/manager-core": "^1.2.0",
    "@ovh-ux/manager-telecom-styles": "^2.0.2",
    "@ovh-ux/manager-telephony": "^0.0.0",
    "@ovh-ux/ng-q-allsettled": "1.0.0-alpha.0",
    "angular": "^1.7.5",
    "font-awesome": "4.7.0",
    "jquery": "^2.1.3",
    "lodash": "^3.10.1",
    "ovh-manager-webfont": "^1.0.2",
    "ui-select": "^0.19.8"
  },
  "devDependencies": {
    "@ovh-ux/manager-webpack-config": "^3.0.1",
    "cypress": "^3.1.3",
    "eslint-plugin-cypress": "^2.1.3",
    "webpack-merge": "^4.1.4"
  }
}

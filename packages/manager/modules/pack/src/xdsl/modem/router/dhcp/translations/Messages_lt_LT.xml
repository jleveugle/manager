   <translation id="xdsl_modem_dhcp_primary_dns" qtlid="311817">Pirminis DNS</translation>
   <translation id="xdsl_modem_dhcp_secondary_dns" qtlid="170408">Antrinis DNS</translation>
   <translation id="xdsl_modem_dhcp_dns_domain" qtlid="311830">DHCP domenas</translation>

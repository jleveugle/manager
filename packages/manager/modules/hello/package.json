    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/manager-hello' --include-filtered-dependencies -- yarn run dev:watch",
    "prepare": "yarn run build"
  },
  "devDependencies": {
    "@ovh-ux/component-rollup-config": "5.0.0-beta.5"
